if MainUI.Parent.Menu.MainMenuUI.Banned.Visible == true then
	Banned = true
	local BannedUI = MainUI.Parent.Menu.MainMenuUI.Banned
	BannedUI.Splash.Image = "rbxassetid://18423397075"
	BannedUI.Splash.Splash.Visible = false
